{
  "author": "Craig Walker",
  "description": "Type Management Utilities for TypeScript",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^4.0.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "typescript",
    "types"
  ],
  "license": "MIT",
  "name": "typonomy",
  "scripts": {
    "check": "tsc --noEmit",
    "declare": "tsc -p tsconfig.declare.json",
    "build": "node esbuild.mjs",
    "docs": "typedoc && rm docs/README.md",
    "lint": "eslint 'src/**/*.ts'",
    "pub": "scripts/pub.sh",
    "test": "jest",
    "validate": "pnpm run check && pnpm run lint && pnpm run test"
  },
  "type": "module",
  "types": "index.d.ts",
  "version": "0.1.7"
}