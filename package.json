{
  "author": "Craig Walker",
  "description": "Type Management Utilities for TypeScript",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/recommended": "^1.0.6",
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitest/coverage-v8": "^1.6.0",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^4.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index",
      "require": "./dist/cjs/index",
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/browser/index"
    },
    "./array": {
      "import": "./dist/esm/array",
      "require": "./dist/cjs/array",
      "types": "./dist/esm/array.d.ts",
      "default": "./dist/browser/array"
    },
    "./func": {
      "import": "./dist/esm/func",
      "require": "./dist/cjs/func",
      "types": "./dist/esm/func.d.ts",
      "default": "./dist/browser/func"
    },
    "./nullish": {
      "import": "./dist/esm/nullish",
      "require": "./dist/cjs/nullish",
      "types": "./dist/esm/nullish.d.ts",
      "default": "./dist/browser/nullish"
    },
    "./object": {
      "import": "./dist/esm/object",
      "require": "./dist/cjs/object",
      "types": "./dist/esm/object.d.ts",
      "default": "./dist/browser/object"
    }
  },
  "keywords": [
    "typescript",
    "types"
  ],
  "license": "MIT",
  "main": "dist/cjs/index",
  "module": "dist/esm/index",
  "name": "typonomy",
  "scripts": {
    "build": "node esbuild.mjs",
    "check": "tsc --noEmit && tsc --noEmit -p test/tsconfig.json",
    "docs": "typedoc && rm docs/README.md",
    "lint": "eslint src/**/*.ts test/**/*.test.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.test.ts",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "pnpm run check && pnpm run lint && pnpm run test"
  },
  "types": "dist/esm/index.d.ts",
  "version": "0.6.2"
}
